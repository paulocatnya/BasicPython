TRABALHANDO COM LISTAS

pares = [18,2,4,6,8,10]
pares.sort(reverse=True)
pares.reverse()



frutas = ["morango","melancia","goiaba","caju"]

del pares[4:]

pares.append(12)


print(pares)
print(frutas)
for item in pares:
    print(item)

print(pares[0])
print(len(pares))
if "goiaba" in frutas:
    print("Tem goiaba!")




